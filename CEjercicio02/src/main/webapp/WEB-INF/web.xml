<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ControllerAjax</servlet-name>
  	<display-name>ControllerAjax</display-name>
  	<description></description>
  	<servlet-class>com.roberto.controllers.ControllerAjax</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Controllerfiles</servlet-name>
  	<display-name>Controllerfiles</display-name>
  	<description></description>
  	<servlet-class>com.roberto.controllers.Controllerfiles</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControllerAjax</servlet-name>
  	<url-pattern>/ControllerAjax</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Controllerfiles</servlet-name>
  	<url-pattern>/Controllerfiles</url-pattern>
  </servlet-mapping>
</web-app>
